<header>
  <h1 class="inline">Pushapage</h1>
    <ul id="nav" class="blue_coloured_element round_bottom"> 
          <% if signed_in? == false %>
             <li><a href="/sign_in">Sign in</a></li>
          <% end %>

	  <% if signed_in? %>
            <li><%= link_to "Home", root_path %></li>
            <li><div style="margin: 10px 5px;">Settings</div>
                <ul>
                <li><%= link_to "Account", edit_user_registration_path %>
                <li><%= link_to "Setup", setup_path %></li>
                </ul>
            </li>
	  <% end %>
          <% if signed_in? %>
            <li><%= link_to "Sign out", '/sign_in_again' %></li>
          <% else %>
          <% end %>
      </ul>
</header>
